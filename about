```markdown
<!-- Hi Everyone, I`m Nikita -->
<h1 style="color:blue; font-size:40px;">Hi Everyone, I`m Nikita</h1>

<!-- Nice and understandable text -->
<p>This is a nice and understandable text.</p>

<!-- Animation -->
<script>
  setTimeout(function() {
    var text = document.querySelector('h1');
    var str = text.textContent;
    var i = 0;
    var timer = setInterval(function() {
      text.textContent = str.slice(0, i);
      i++;
      if (i > str.length) {
        clearInterval(timer);
        setTimeout(function() {
          text.textContent = "Go to my profile";
        }, 1000);
      }
    }, 100);
  }, 5000);
</script>
